<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StackView">
        <attr name="stackview_rotationRandomMagnitude" format="integer" />
        <!-- Must be greater than 0 and less than 1 -->
        <attr name="stackview_horizontalScalingFactor" format="float"/>
        <attr name="stackview_horizontalSpacing" format="dimension" />
        <attr name="stackview_verticalSpacing" format="dimension" />
        <attr name="stackview_cyclic" format="boolean" />
        <attr name="stackview_size" format="integer" />
        <attr name="stackview_adapterLayout" format="reference" />
    </declare-styleable>
</resources>